{
  "name": "seqrets",
  "version": "1.3.7",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:crypto": "npm run build --workspace=@seqrets/crypto",
    "dev": "npm run build:crypto && next dev --port 9002",
    "build": "npm run build:crypto && next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "security-audit": "npm audit",
    "security-audit:fix": "npm audit fix",
    "deps:outdated": "npm outdated",
    "deps:update": "npm update",
    "build:analyze": "ANALYZE=true npm run build",
    "desktop:dev": "npm run build:crypto && npm run tauri:dev:fresh --workspace=@seqrets/desktop",
    "desktop:build": "npm run build:crypto && npm run tauri:build --workspace=@seqrets/desktop"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@noble/ciphers": "0.4.0",
    "@noble/hashes": "^1.4.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@scure/bip39": "^1.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "jsqr": "^1.4.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.475.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.0",
    "pako": "^2.1.0",
    "qrcode": "^1.5.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^9.0.1",
    "rehype-sanitize": "^6.0.0",
    "shamirs-secret-sharing-ts": "^1.0.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "20.11.20",
    "@types/pako": "^2.0.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "5.3.3"
  },
  "overrides": {
    "qs": "^6.14.1",
    "fast-xml-parser": "5.3.4"
  }
}
